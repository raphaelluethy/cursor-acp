{
  "name": "@raphael/cursor-acp",
  "version": "0.1.0",
  "description": "ACP adapter backed by Cursor Agent CLI",
  "main": "dist/lib.js",
  "types": "dist/lib.d.ts",
  "bin": {
    "cursor-acp": "./dist/index.js"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/lib.d.ts",
      "import": "./dist/lib.js"
    },
    "./*": "./*"
  },
  "files": [
    "dist/",
    "!dist/tests/",
    "README.md",
    "LICENSE",
    "NOTICE",
    "package.json"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "bun run build && node dist/index.js",
    "lint": "oxlint src",
    "lint:fix": "oxlint src --fix",
    "lint:eslint": "eslint src --ext .ts",
    "lint:eslint:fix": "eslint src --ext .ts --fix",
    "format": "oxfmt --config .oxfmtrc.json --write src",
    "format:check": "oxfmt --config .oxfmtrc.json --check src",
    "format:prettier": "prettier --write .",
    "format:prettier:check": "prettier --check .",
    "typecheck": "tsgo --noEmit",
    "check": "bun run lint && bun run format:check",
    "check:legacy": "bun run lint:eslint && bun run format:prettier:check",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "cursor",
    "acp",
    "agent",
    "typescript",
    "adapter"
  ],
  "license": "Apache-2.0",
  "dependencies": {
    "@agentclientprotocol/sdk": "0.13.1",
    "@typescript/native-preview": "^7.0.0-dev.20260204.1",
    "oxfmt": "^0.28.0",
    "oxlint": "^1.43.0"
  },
  "devDependencies": {
    "@types/node": "25.2.0",
    "@typescript-eslint/eslint-plugin": "8.54.0",
    "@typescript-eslint/parser": "8.54.0",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "globals": "17.3.0",
    "oxlint-tsgolint": "^0.11.4",
    "prettier": "3.8.1",
    "typescript": "5.9.3",
    "vitest": "4.0.18"
  }
}
